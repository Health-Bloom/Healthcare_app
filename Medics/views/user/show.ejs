<%- include('../base/header')%>

<%- include('../base/navbar')%>

<div class="row">
	<div class="col-md-4">
			<h1><%= user.username %></h1>
		<div class="thumbnail">
			<%if(Contribute.images.length) {%>
				<img class="img-fluid" style="border-radius: 20px;" alt="" src="<%=Contribute.images[0].url%>">
				<% }else {%>
				<img class="img-fluid" style="border-radius: 20px;" alt=""
					src="">
				<% } %>
			<img src="<%= user.avatar %>" alt="user profile image">
			<div class="caption"><a href="mailto:<%= user.email %>"><%= user.email %></a></div>
		</div>

        <!-- <div class="border border-primary shadow" id='map' ></div> -->

	</div>
	<div class="col-md-8">
		<h3><%= user.username %>'s Medicine:</h3>
		<ul>
		<% Contributes.forEach(function(Contribute) { %>
			<li><%= Contribute.medname %></li>
		<% }); %>
		</ul>
	</div>
</div>

<%- include('../base/footer')%>
